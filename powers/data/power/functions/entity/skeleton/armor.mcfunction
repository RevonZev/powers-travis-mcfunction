#> Execute as player that have power.skeleton tag and replace their armor

replaceitem entity @a[tag=power.skeleton] armor.head minecraft:skeleton_skull
replaceitem entity @a[tag=power.skeleton] armor.chest minecraft:leather_chestplate{display:{color:13619151}}
replaceitem entity @a[tag=power.skeleton] armor.legs minecraft:leather_leggings{display:{color:13619151}}
replaceitem entity @a[tag=power.skeleton] armor.feet minecraft:leather_boots{display:{color:13619151}}